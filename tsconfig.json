{
    "compilerOptions": {
        "target": "ES2018",
        "module": "commonjs",
        "strict": false,
        "skipLibCheck": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "declaration": false,
        "noImplicitAny": false
    },
    "include": ["src/**/*"]
}
